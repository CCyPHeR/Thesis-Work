issn = {00134686},
journal = {Electrochimica Acta},
keywords = {Anode effect,Bubble coverage,Gas-evolving electrodes,Surface passivation,Wettability},
pages = {611--618},
publisher = {Elsevier Ltd},
title = {{On the various types of uncontrolled potential increase in electrochemical reactors - The anode effect}},
url = {http://dx.doi.org/10.1016/j.electacta.2012.09.051},
volume = {87},
year = {2013}
}
