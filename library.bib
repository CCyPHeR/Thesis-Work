keywords = {CFD,Electrochemical reactors,Electrogenerated bubbles,Electrolysis,Hydrodynamics,Metrology},
pages = {268--281},
title = {{Electrogenerated bubbles induced convection in narrow vertical cells: A review}},
volume = {100},
year = {2015}
}
